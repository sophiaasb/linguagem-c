#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <limits.h>

int main() {
    setlocale(LC_ALL, "portuguese");

    int i, numeros[5];
    int qntdPares = 0, qntdImpares = 0;
    int qntdPos = 0, qntdNeg = 0;
    int maior, menor;
    float mediaTotal = 0, mediaPares = 0, mediaImpares = 0;

// qntd números inseridos
    for (i = 0; i < 5; i++) {
         printf("Digite o %iº valor: ", i+1);
         scanf("%d", &numeros[i]);
}

   // qntd pares e impares 
   if (numeros[i] % 2 == 0) {
         qntdPares++;
      } else if {
         qntdImpares++;	
}
		
   // qntd positivos e negativos
   if (numeros[i] < 0) {
	 qntdNeg++;
       } else {
         qntdPos++;
}
	
 // maior e menor
for (i = 1; i < 5; i++) {
if (numerosTT[i] < menor) {
menor = numerosTT[i];
} else if (numeros[i]) > maior {
    maior = numeros[i];
}
}

// media pares

// media impares

// media numeros 

    printf("=== Exibindo resultados ===\n");
    printf("Quantidade de números pares: %d\n", qntdPares);
    printf("Quantidade de números ímpares: %d\n", qntdImpares);
    printf("Quantidade de números positivos: %d\n", qntdPos);
    printf("Quantidade de números negativos: %d\n", qntdNeg);
    printf("Quantidade de números inseridos: %d\n", numeros);
    printf("Maior número: %d\n", maior);
    printf("Menor número: %d\n", menor);
    printf("Média dos números pares: %.2f\n", mediaPares);
    printf("Média dos números ímpares: %.2f\n", mediaImpares);
    printf("Média de números inseridos: %.2f\n", mediaTotal);
    printf("Números lidos na ordem inversa:\n");  // numeros na ordem inversa
    for (i = 4; i >= 0; i--) {
         printf("%d", numeros[i]);
    }

    return 0;
}
