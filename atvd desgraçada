#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <limits.h>

int main() {
    setlocale(LC_ALL, "portuguese");

    int i, numeros[5];
    int qntdPares = 0, qntdImpares = 0;
    int qntdPos = 0, qntdNeg = 0;
    int maior, menor;
    float mediaTotal = 0, mediaPares = 0, mediaImpares = 0;
     
    for (i = 0; i < TAM; i++) {
    printf("Informe o valor dos 5 números");
    scanf("%i", &numeros[i]);

   // qntd pares e impares 
if (numeros[i] % 2 == 0) {
      qntdPares++;
} else if {
      qntdImpares++;	
}
		
   // qntd positivos e negativos
if (numeros[i] < 0) {
	qntdNeg++;
} else {
       qntdPos++;
}
	
// qntd numeros
 for(i = 0; i < 5; i++) {
   	printf("Digite o %iº valor: ", i+1);
   	scanf("%d", &numeros[i]);
   	
   if(numeros[i] < 0) {
   numeros[i] = 0;
}
}
   
   for(i = 0; i < 5; i++) {
   	printf("O valor %iº digitado é: %d \n", i+1, numeros[i]);
        printf("Números inseridos: %i \n", quantidadeNumeros);
   }
   
	return 0;
}

 // maior e menor
for (i = 1; i < 5; i++) {
if (numerosTT[i] < menor) {
menor = numerosTT[i];
} else if (numeros[i]) > maior {
    maior = numeros[i];
}
}

// media pares

// media impares

// media numeros 

// numeros na ordem inversa
for (i = 10; i >= 1; i--) {
		printf("%d \n", i);
	}
	return 0;
}
