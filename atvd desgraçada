#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <locale.h>
#include <limits.h>

#define TAM 5

int main() {
    setlocale(LC_ALL, "portuguese");
    int qntdNPares = 0, qntdNImpares = 0;
    int qntdNPosi = 0, qntdNNega = 0;
    int numerosTT[i];
    int maior, menor;
    float mediaPares, mediaImpares;
    float mediaTT;
    int reverso;
    int i, j, numeros[MAX];
     
// qntd pares e impares 
    for ( i = 0; i < TAM; i++){
    printf("Informe o valor dos 5 números");
    scanf("%i", &numerosTT[i]);
    	
     if (numerosTT[i] % 2 == 0) {
	 qntdNPares++;
	 
	} else if{
		qntdNImpares++;
		
		
	// qntd positivos e negativos
	} if (numerosTT[i] < 0){
		qntdNNega++;
			
	} else {
		qntdNPosi++;
	}
		}
	
// qntd numeros
 for(i = 0; i < MAX; i++) {
   	printf("Digite o %iº valor: ", i+1);
   	scanf("%d", &numeros[i]);
   	fflush(stdin);
   	
   	if(numeros[i] < 0) {
   		numeros[i] = 0;
	   }
   }
   
   for(i = 0; i < MAX; i++) {
   	printf("O valor %iº digitado é: %d \n", i+1, numeros[i]);
   }
   
	return 0;
}

// maior e menor
	for (i = 1; i < TAM; i++) {
		if (numerosTT[i] < menor) {
			menor = numerosTT[i];
			}
		if (numerosTT[i]) > maior {
		    maior = numerosTT[i];
		}
	}


// media pares

// media impares

// media numeros 

// numeros na ordem inversa
	for (j = 10; j >= 1; j--) {
		printf("%d \n", j);
	}
	return 0;
}
